         </Text>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-with-shadow" text="Samuel Gamage">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Text>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-with-shadow" text="Isaac King">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Text>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-with-shadow" text="A'maya Solomon">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Text>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-with-shadow" text="Yeajin Shin">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Text>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-with-shadow" text="Jessie Sui">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </Text>
            <Button mnemonicParsing="false" onAction="#handleStartOverClick" text="Play Again">
                <VBox.margin>
                    <Insets top="20.0" />
                </VBox.margin>
            </Button>
        </VBox>
    </center>
</BorderPane>
